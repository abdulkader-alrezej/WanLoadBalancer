
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option packet_steering  '2'

config device
	option name 'br-wlan'
	option type 'bridge'
	list ports 'phy0-ap0'
	list ports 'phy1-ap0:'

config interface 'wlan'
	option proto 'static'
	option ipaddr '192.168.50.1'
	option netmask '255.255.255.0'
	option device 'br-wlan'

config interface 'lan'
	option device 'p5'
	option proto 'static'
	option ipaddr '192.168.40.1'
	option netmask '255.255.255.0'
	option ip6assign '0'

config interface 'wan1'
	option proto 'dhcp'
	option device 'p1'
	option metric '101'
	option ipv6 '0'
	option delegate '0'
	option hostname '*'
	option comment 'up'





