
config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/soc/a000000.wifi'
	option band '2g'
	option channel '6'
	option htmode 'HT20'
	option txpower '30'
	option cell_density '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'wlan'
	option mode 'ap'
	option ssid 'ArabRouter_2.4'
	option encryption 'psk2'
	option isolate '1'
	option key '12345678'
	option disabled '0'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/soc/a800000.wifi'
	option band '5g'
	option channel '100'
	option htmode 'VHT80'
	option txpower '23'
	option cell_density '0'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option network 'wlan'
	option mode 'ap'
	option ssid 'ArabRouter_5'
	option encryption 'psk2'
	option isolate '1'
	option key '12345678'
	option disabled '0'

